    "test": "npm run build && sudo node ./test/Connection.spec.js && echo Test Completed",
    
    "wss" : "sudo node wss.js"
  },
  "engines": {
    "node": ">= 8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ApexCaptain/GanglionManager.git"
  },
  "keywords": [
    "openbci",
    "eeg",
    "neurojs",
    "ganglion"
  ],
  "author": "",
