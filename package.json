{
  "name": "ganglion-manager",
  "version": "1.0.5",
  "description": "Ganglion Board EEG data receiver",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node ./dev/ClearTask.js && tsc && echo Build Completed",
    "push": "node ./dev/GitPushingTask.js -m",
