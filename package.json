  "bugs": {
    "url": "https://github.com/ApexCaptain/GanglionManager/issues"
  },
  "homepage": "https://github.com/ApexCaptain/GanglionManager#readme",
  "devDependencies": {
    "@types/node": "^14.14.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/events": "^3.0.0",
    "@types/ws": "^7.4.0",
    "eeg-pipes": "^2.0.3",
    "koconut": "^1.0.15",
    "openbci-observable": "^1.0.5",
    "serialport": "^9.0.2",
    "ws": "^7.4.4"
  }
